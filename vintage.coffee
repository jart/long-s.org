# execute AFTER long s pass
ligatures = (w) ->
  """Replace normal letters with unicode ligatures"""
  for lig in ligs
    while -1 isnt i = w.search lig[0]
      w = w[...i] + lig[1] + w[i + lig[2]...]
  w


