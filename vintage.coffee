String.prototype.endsWith = (suffix) ->
  """Determine if string ends with one or more values"""
  if Array.isArray suffix
    for s in suffix
      return true if @endsWith s
    false
  else
    -1 isnt @indexOf suffix, @length - suffix.length


String.prototype.startsWith = (prefix) ->
  """Determine if string starts with one or more values"""
  if Array.isArray prefix
    for s in prefix
      return true if @startsWith s
    false
  else
    0 is @indexOf prefix


oneword = (tbl) ->
  """Transpose one words from lookup table"""
  _oneword = (w) ->
    if w.length == 1
      return w
    allcaps = (w is w.toUpperCase())
    wl = w.toLowerCase()
    if tbl[w]
      # try normal casing first
      res = tbl[w]
    else if tbl[wl]
      # then try lowercased version
      res = tbl[wl]
    else
      res = w
      # unless there's a suffix to strip and try again (unless vowel ending)
      for suffix in suffixes
        if wl.endsWith suffix
          without = wl[...-suffix.length]
          newword = _oneword(without)
          if newword isnt without
            if not (newword.endsWith vowels and suffix.startsWith vowels)
              res = newword + suffix
              break
    if allcaps
      # re-capitalize if it was all caps
      res = res.toUpperCase()
    else if w[0] is w[0].toUpperCase() and w.length > 2 and res.length > 2
      # if it was capitalized, put it back
      if res[0] in apostrophes
        res = res[0] + res[1].toUpperCase() + res[2...]
      else
        res = res[0].toUpperCase() + res[1...]
    res


multiword = (tbl) ->
  """Transpose one or more words from lookup table"""
  # {'hi there': 'yo dawg'} -> {
  #   end: null,
  #   more: {
  #     'hi': {
  #       end: null,
  #       more: {
  #         'there': {
  #           end: ['yo', 'dawg'],
  #           more: {},
  #         }
  #       }
  #     }
  #   }
  # }
  tbl2 = {end: null, more: {}}
  for k, v of tbl
    t = tbl2
    for w in k.split /\ /
      if not t.more[w]
        t.more[w] = {end: null, more: {}}
      t = t.more[w]
    t.end = v.split /(\ )/
  tbl = tbl2
  _multiword = (toks, i) ->
    if toks[i].length < 2
      return
    t = tbl
    start = i
    end = -1
    while toks[i] and t.more
      if toks[i].length > 1
        if t.more[toks[i]]
          t = t.more[toks[i]]
          end = i
        else
          break
      else if toks[i] not in whitespace
        break
      i++
    if t.end and end isnt -1
      toks[start..end] = t.end
  _multiword.multiword = true
  _multiword


long_s = (w) ->
  """Replace s with long s (ſ) when appropriate

  Rules:

  - Short s is used at the end of a word (e.g. his, complains, ſucceſs)

  - Short s is used before an apostrophe (e.g. clos'd, us'd)

  - Short s is used before the letter 'f' (e.g. ſatisfaction, misfortune,
    transfuſe, transfix, transfer, ſucceſsful)

  - Short s is used after the letter 'f' (e.g. offset), although not if the
    word is hyphenated (e.g. off-ſet)

  - Short s is used before the letter 'b' in books published during
    the 17th century and the first half of the 18th century (e.g. husband,
    Shaftsbury), but long s is used in books published during the second
    half of the 18th century (e.g. huſband, Shaftſbury)

  - Short s is used before the letter 'k' in books published during
    the 17th century and the first half of the 18th century (e.g. skin, ask,
    risk, masked), but long s is used in books published during the second
    half of the 18th century (e.g. ſkin, aſk, riſk, maſked)

  - Compound words with the first element ending in double s and the second
    element beginning with s are normally and correctly written with a
    dividing hyphen (e.g. Croſs-ſtitch, Croſs-ſtaff), but very occasionally
    may be written as a single word, in which case the middle letter 's' is
    written short (e.g. Croſsſtitch, croſsſtaff).

  - Long s is used initially and medially except for the exceptions noted
    above (e.g. ſong, uſe, preſs, ſubſtitute)

  - Long s is used before a hyphen at a line break (e.g. neceſ-ſary,
    pleaſ-ed), even when it would normally be a short s (e.g. Shaftſ-bury
    and huſ-band in a book where Shaftsbury and husband are normal),
    although exceptions do occur (e.g. Mans-field)

  - Short s is used before a hyphen in compound words with the first element
    ending in the letter 's' (e.g. croſs-piece, croſs-examination,
    Preſs-work, bird's-neſt)

  - Long s is maintained in abbreviations such as ſ. for ſubſtantive, and
    Geneſ. for Geneſis (this rule means that it is practically impossible to
    implement fully correct automatic contextual substitution of long s at
    the font level)

  Source: http://babelstone.blogspot.com/2006/06/rules-for-long-s.html
  """
  res = []
  for i in [0...w.length - 1]
    if w[i] is 's'
      if w[i + 1] in ["'", "’", "`", "f", "b", "k", "-"]
        res.push 's'
      else if w[i - 1] is 'f'
        res.push 's'
      else
        res.push 'ſ'
    else
      res.push w[i]
  res.push w[w.length - 1]
  res.join ''


