punctuation = (toks, i) ->
  """Replace ASCII representations of proper unicode punctuation characters"""
  w = toks[i]
  # ellipses
  if (toks[i] is '.' and
      toks[i + 1] is '.' and
      toks[i + 2] is '.' and
      toks[i + 3] isnt '.')
    toks[i...i + 3] = "…"
  # em dash
  else if -1 isnt o = w.indexOf '---'
    if w[o - 1] isnt '-' and w[o + 3] isnt '-'
      toks[i] = w[...o] + "—" + w[o + 3...]
  # en dash
  else if -1 isnt o = w.indexOf '--'
    if w[o - 1] isnt '-' and w[o + 2] isnt '-'
      toks[i] = w[...o] + "–" + w[o + 2...]
punctuation.multiword = true


dsplit = (text) ->
  """Tokenize some text"""
  toks = /(\.|\!|\?|\,|\;|\:|\"|\“|\”|\(|\)|\–|\—|\…|\[|\]|\<|\>| |\t|\n)/
  (s for s in text.split(toks) when s)


def_transforms = [
  middle_english, punctuation, spellings, unamerican, long_s, ligatures
  # contractions, unamerican, long_s, ligatures, punctuation
]

all_transforms = [
  middle_english, punctuation, spellings, contractions, unamerican
  long_s, ligatures
]

translate = (text, transforms = def_transforms) ->
  """Breaks text into words and applies word transform functions"""
  tokens = dsplit(text)
  i = 0
  while i < tokens.length
    for transform in transforms
      if transform.multiword
        transform tokens, i
      else
        tokens[i] = transform tokens[i]
    i++
  tokens.join('')


exports?.translate = translate

